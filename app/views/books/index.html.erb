<div class="container">
  <div class="row">

  <div class="col-xs-3">
    <%= render 'users/user', users: @users %>
  </div>

  <div class="col-xs-9">

<h4>Books</h4>

<table class="table table-hover table-inverse">
    <thead>
      <tr>
        <th>Title</th>
        <th>Opinion</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @books.each do |book| %>
      <tr>
        <td><%= link_to book.title, book_path(book) %></td>
        <td>test</td>
      </tr>
     <% end %>
    </tbody>
  </table>
  </div>
 </div>
</div>